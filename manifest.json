{
  "manifest_version": 3,
  "name": "SelectGPT",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://*.aman0.com/*",
    "<all_urls>"
  ],
  "action": {
    "default_icon": "icon-on.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}